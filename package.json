{
    "name": "ClientCraft",
    "private": "true",
    "version": "0.0.0",
    "type": "commonjs",
    "description": "proxy package.json, so NPM Taskrunner extension in visual studio can find the commands",
    "scripts": {
        "build": "cd ./ClientCraft.Client/Assets/ && npm run build",
        "watch": "cd ./ClientCraft.Client/Assets/ && npm run watch",
        "dotnet": "cd ./ClientCraft.Website/ && dotnet run"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "rollup-plugin-copy": "^3.5.0"
    },
    "dependencies": {
        "@faker-js/faker": "^9.2.0",
        "@umbraco-ui/uui": "^1.11.0",
        "dotenv": "^16.4.5",
        "faker": "^6.6.6"
    }
}
